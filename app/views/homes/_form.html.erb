<div class="centered">
  <h1>Create your new home</h1>
  <%= form_tag homes_path, :method => :get do %>
      <%= text_field_tag :search, params[:search],:id => 'geo-input' %>
  <% end %>
<%= form_for @home, url: homes_path, html: { multipart: true } do |f| %>
  <%= f.label :Photo %>
  <%= f.file_field :avatar %>
  <br>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br>
  <%= f.label :phone_number %>
  <%= f.text_field :phone_number %>
  <br>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <br>
  <%= f.label :pet_type %>
  <%= f.text_field :type_pet %>
  <br>
  <%= f.file_field :avatar %>
  <br>
  <%= f.submit 'Save' %>

<% end %>
